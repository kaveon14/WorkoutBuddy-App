<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <android.support.constraint.ConstraintLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="0dp">

        <TextView
            android:id="@+id/mainWorkout_textView"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:text="MainWorkout ->"
            android:textSize="15sp"
            android:textColor="@android:color/black"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintBottom_creator="1"
            android:layout_marginStart="16dp"
            app:layout_constraintBottom_toBottomOf="@+id/subWorkout_textView"
            android:layout_marginTop="46dp"
            tools:layout_constraintLeft_creator="1"
            android:layout_marginBottom="43dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="@+id/liftingStatsDate_textView" />

        <TextView
            android:id="@+id/subWorkout_textView"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:text="SubWorkout ->"
            android:textSize="15sp"
            android:textColor="@android:color/black"
            tools:layout_constraintTop_creator="1"
            android:layout_marginStart="16dp"
            android:layout_marginTop="186dp"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/liftingStatsDate_textView"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:text="Date -> "
            android:textSize="15sp"
            android:textColor="@android:color/black"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toTopOf="@+id/sets_textView"
            android:layout_marginStart="16dp"
            android:layout_marginTop="97dp"
            tools:layout_constraintLeft_creator="1"
            android:layout_marginBottom="100dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/sets_textView"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:text="Total Sets ->"
            android:textSize="15sp"
            android:textColor="@android:color/black"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintBottom_creator="1"
            android:layout_marginStart="16dp"
            app:layout_constraintBottom_toBottomOf="@+id/reps_textView"
            android:layout_marginTop="41dp"
            tools:layout_constraintLeft_creator="1"
            android:layout_marginBottom="42dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="@+id/subWorkout_textView" />

        <TextView
            android:id="@+id/reps_textView"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:text="Total Reps -> "
            android:textSize="15sp"
            android:textColor="@android:color/black"
            tools:layout_constraintTop_creator="1"
            android:layout_marginStart="16dp"
            android:layout_marginTop="53dp"
            app:layout_constraintTop_toBottomOf="@+id/subWorkout_textView"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="parent" />

        <TextView
            android:id="@+id/weight_textView"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:text="Total Weight ->"
            android:textSize="15sp"
            android:textColor="@android:color/black"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintBottom_creator="1"
            android:layout_marginStart="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="168dp"
            tools:layout_constraintLeft_creator="1"
            android:layout_marginBottom="170dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="@+id/mainWorkout_textView" />

    </android.support.constraint.ConstraintLayout>
</RelativeLayout>
